# frozen_string_literal: true

active_admin_form_for [:admin, resource] do |f|
  f.inputs do
    f.input :nom
    if can?(:manage, Compte)
      f.input :parent_id, as: :select, collection: StructureAdministrative.all
    end
  end
  f.actions do
    f.action :submit
    annulation_formulaire(f)
  end
end
